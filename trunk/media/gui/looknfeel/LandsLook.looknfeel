                    <Dim type="RightEdge" >
                        <UnifiedDim scale="1" type="RightEdge">
                            <DimOperator op="Subtract">
                                <ImageDim imageset="LandsLook" image="SysAreaRight" dimension="Width">
                                    <DimOperator op="Add">
                                        <ImageDim imageset="LandsLook" image="SysAreaMiddle" dimension="Width" />
                                    </DimOperator>
                                </ImageDim>
                            </DimOperator>
                        </UnifiedDim>
                    </Dim>
