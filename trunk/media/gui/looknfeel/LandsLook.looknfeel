            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="Width" ><ImageDim imageset="LandsLook" image="HeaderBarSortUp" dimension="Width" /></Dim>
                    <Dim type="Height" ><ImageDim imageset="LandsLook" image="HeaderBarSortUp" dimension="Height" /></Dim>
                </Area>
                <Image imageset="LandsLook" image="HeaderBarSortUp" />
            </ImageryComponent>
        </ImagerySection>
