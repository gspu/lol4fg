            <key name="Attack" main="MB_Left" alt="KC_UNASSIGNED" />
            <key name="Spellcast" main="MB_Right" alt="KC_UNASSIGNED" />
            <key name="Run" main="KC_LSHIFT" alt="KC_UNASSIGNED" />
            <key name="Action" main="KC_E" alt="KC_UNASSIGNED" />
            <key name="Inventory" main="KC_I" alt="KC_TAB" />
            <key name="InvTakeOne" main="MB_Left" alt="KC_UNASSIGNED" />
            <key name="InvTakeAll" main="MB_Middle" alt="KC_UNASSIGNED" />
            <key name="InvUse" main="MB_Right" alt="KC_UNASSIGNED" />
            <key name="Spellbook" main="KC_M" alt="KC_UNASSIGNED" />
            <key name="Screenshot" main="KC_SYSRQ" alt="KC_UNASSIGNED" />
            <key name="Mainmenu" main="KC_ESCAPE" alt="KC_UNASSIGNED" />
            <key name="LookUp" main="KC_PGUP" alt="KC_UNASSIGNED" />
            <key name="LookDown" main="KC_PGDOWN" alt="KC_UNASSIGNED" />
            <key name="LookLeft" main="KC_LEFT" alt="KC_NUMPAD4" />
            <key name="LookRight" main="KC_RIGHT" alt="KC_NUMPAD6" />
            <key name="ToggleMouselooking" main="KC_X" alt="KC_UNASSIGNED" />
            <key name="Debug3rdp" main="KC_F1" alt="KC_UNASSIGNED" />
            <key name="Debugshowlines" main="KC_F2" alt="KC_UNASSIGNED" />
        </keys>
    </settings>
</game>
