<game>
    <Renderer selected="Direct3D9 Rendering Subsystem">
        <RenderSystem name="Direct3D9 Rendering Subsystem">
            <option name="Allow NVPerfHUD" value="No" />
            <option name="FSAA" value="0" />
            <option name="Floating-point mode" value="Fastest" />
            <option name="Full Screen" value="No" />
            <option name="Rendering Device" value="Monitor-1-NVIDIA GeForce 8800 GTS" />
            <option name="Resource Creation Policy" value="Create on all devices" />
            <option name="VSync" value="No" />
            <option name="VSync Interval" value="1" />
            <option name="Video Mode" value="800 x 600 @ 32-bit colour" />
            <option name="sRGB Gamma Conversion" value="No" />
        </RenderSystem>
        <RenderSystem name="OpenGL Rendering Subsystem">
            <option name="Colour Depth" value="32" />
            <option name="Display Frequency" value="N/A" />
            <option name="FSAA" value="0" />
            <option name="Full Screen" value="No" />
            <option name="RTT Preferred Mode" value="FBO" />
            <option name="VSync" value="No" />
            <option name="VSync Interval" value="1" />
            <option name="Video Mode" value="800 x 600" />
            <option name="sRGB Gamma Conversion" value="No" />
        </RenderSystem>
    </Renderer>
    <settings>
        <options>
            <option name="mouselooking" value="true" />
            <option name="sensibility_x_mouse" value="0.16" />
            <option name="sensibility_y_mouse" value="0.14" />
            <option name="physics_fps" value="60" />
            <option name="sensibility_x_keyboard" value="1" />
            <option name="gain_all" value="1" />
            <option name="gain_sfx" value="1" />
            <option name="gain_speech" value="1" />
            <option name="gain_music" value="1" />
            <option name="gain_player_sfx" value="1" />
            <option name="rolloff_factor" value="1" />
            <option name="ref_distance" value="150" />
            <option name="max_distance" value="3400" />
            <option name="distance_model" value="53250" />
            <option name="starting_level" value="a_start.zip" />
            <option name="starting_entrance" value="entr0" />
            <option name="default_music" value="default.ogg" />
            <option name="default_combat_music" value="default.ogg" />
        </options>
        <keys>
            <key name="MoveForward" main="KC_W" alt="KC_NUMPAD8" />
            <key name="MoveBack" main="KC_S" alt="KC_NUMPAD5" />
            <key name="MoveRight" main="KC_D" alt="KC_UNASSIGNED" />
            <key name="MoveLeft" main="KC_A" alt="KC_UNASSIGNED" />
